{
  "name": "sk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../pb && modd",
    "backend": "AUDITLOG=users,posts ../pb/entrypoint.sh ./pocketbase serve --publicDir=../sk/build",
    "backend:windows": "cd ../pb && set \"RELEASE=standard\" && entrypoint.bat pocketbase.exe serve --publicDir=../sk/build",
    "build": "vite build",
    "build:backend": "cd ../pb && go build",
    "typegen": "pocketbase-typegen --db ../pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "test": "playwright test",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "pocketbase-typegen": "^1.3.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.87.0",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.7",
    "svelte-preprocess": "^6.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.4"
  },
  "type": "module",
  "dependencies": {
    "@simplewebauthn/browser": "^13.1.0",
    "pocketbase": "^0.26.0",
    "zod": "^3.24.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "esbuild",
      "sqlite3",
      "svelte-preprocess",
      "workerd"
    ]
  }
}
